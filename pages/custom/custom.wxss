}

.recommend-product {
  padding: 24rpx;
  background-color: #fff;
  margin-bottom: 24rpx;
}

.recommend-product-group {
  display: flex;
  flex-direction: row;
}

.group-cell-left {
  width: 416rpx;
  margin-right: 8rpx;
}

.group-cell-left image {
  width: 100%;
  height: 568rpx;
}

.group-cell-right {
  display: flex;
  flex-direction: column;
}

.cell-right-row {
  position: relative;
  width: 282rpx;
  height: 282rpx;
  overflow: hidden;
}

.cell-right-row.top {
  margin-bottom: 8rpx;
}

.cell-right-row image {
  width: 280rpx;
  height: 280rpx;
}

.recommend-price {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 162rpx;
  height: 56rpx;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  line-height: 56rpx;
  text-align: center;
  font-size: 30rpx;
}

.recommend-price text {
  font-family: "微软雅黑";
}

.product-group {
  padding: 50rpx 24rpx 0;
  background-color: #fff;
}

.product-group-title {
  display: inline-block;
  margin-bottom: 44rpx;
}

.title-box {
  text-align: center;
}

.product-group-title .title-content {
  display: flex;
  flex-direction: row;
  height: 36rpx;
}

.product-group-title .title-content .line {
  position: relative;
  display: flex;
  width: 80rpx;
  height: 2rpx;
  background-color: #757575;
  align-items: center;
  margin-top: 20rpx;
}

.product-group-title .title-content .line-left {
  margin-right: 16rpx;
}

.product-group-title .title-content .line:after {
  position: absolute;
  top: -2rpx;
  display: block;
  content: "";
  width: 8rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background-color: #757575;
}

.product-group-title .title-content .line-left:after {
  right: 0;
}

.product-group-title .title-content .line-right {
  margin-left: 16rpx;
}

.title-text {
  color: #395bae;
  font-size: 36rpx;
  line-height: 36rpx;
}

.product-lists:after {
  display: block;
  content: "";
  width: 0;
  height: 0;
  clear: both;
}

.product-lists .product-item {
  width: 340rpx;
  margin-right: 22rpx;
  float: left;
  margin-bottom: 48rpx;
}

.product-lists .product-item:nth-child(2n) {
  margin-right: 0;
}

.product-lists .product-item image {
  display: block;
  width: 340rpx;
  height: 340rpx;
  margin-bottom: 10rpx;
}

.product-lists .product-item .product-name {
  font-size: 28rpx;
  color: #202020;
  line-height: 1.5;
  height: 90rpx;
  overflow: hidden;
  width:100%; 
  /* white-space:nowrap; */
  text-overflow:ellipsis; 
}

.product-item .price {
  position: relative;
  color: #ff5274;
  font-size: 32rpx;
  line-height: 32rpx;
  height: 32rpx;
}
.product-item .product-descr {
  font-size: 24rpx;
  color: #333;
  width:100%; 
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis; 
}
.product-item .icon_buy{
    width: 20px;
    height: 20px;
    background-image: url(http://my-two.yunmof.com/Shop/PublicMob/images/icon_addcart.png);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: 20px;
    position: absolute;
    right: 0;
    top: 0px;
}
.add-cart {
  position: absolute;
  display: block;
  width: 40rpx;
  height: 48rpx;
  right: 20rpx;
  top: 50%;
  transform: translateY(-60%);
  background: url(http://testfile.xiaokeduo.com/system/xkdxcx/system/images/cart-icon.png) no-repeat left top;
  background-size: cover;
}

scroll-view {
  position: fixed;
  left: 0;
  top: 236rpx;
  bottom: 0;
}





