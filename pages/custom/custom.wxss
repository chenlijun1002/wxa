/* 文本导航 */
.text-nav{
  line-height: 64rpx;
  height: 64rpx;
  background-color: #fff;
  border-top: 1rpx solid #f0f0f0;
  padding-left: 24rpx;
  font-size: 24rpx;
}
.nav-text{
  display: inline-block;
  width: 90%;
  white-space: nowrap;  
  text-overflow: ellipsis;
  overflow: hidden;
}
.text-nav image{
  width: 24rpx;
  height: 24rpx;
  float: right;
  margin-top: 20rpx;
  margin-right: 24rpx;
}
.border-bottom{
  border-bottom: 1rpx solid #f0f0f0;
}
/* 活动 */
.temp-activity{
  width: 100%;  
  overflow: hidden;
}
.activity-item{  
  float: left;
  width: 50%;
  position: relative;
  vertical-align: top;
  padding: 10rpx 0;
  /* border: 1rpx solid #E9E9E9;   */
  background-color: #fff;
  box-sizing: border-box;
}
.item-inner{
  padding: 10rpx;
  border: 1px solid #E9E9E9;
  text-align: center;
}
.item-inner image{
  /* height: 312rpx; */
  height: 100%;
  max-width: 100%;
  vertical-align: top;
}
.temp-activity >.activity-item >.item-inner image{
  height: 340rpx;
}
.activity-item:nth-child(odd) {
    padding-right: 10rpx;
}
.activity-item:nth-child(even) {
    padding-left: 10rpx;
}
.activity-text{
  display: block;
    position: relative;
    width: 96%;
    padding: 0;    
    color: #FF5F00;
    border-radius: 0;
    right: 0;
    bottom: 0;
    font-size: 28rpx;
     word-break: break-all;
}
.activity-tag{
    float: left;
    /* display: inline-block;     */
    padding: 0 8rpx;
    height: 40rpx;
    line-height: 36rpx;
    color: #ff9800;
    border: 1rpx solid #ff9800;
    text-align: center;
    font-size: 24rpx;
    border-radius: 8rpx;
    margin-top: 8rpx;
    margin-right: 8rpx;   
}
.activity-price{                 
    color: #929292;
    font-size: 24rpx;
    display: inline-block;
    width: 75%;
}
/* layout2 */
.activity-item-layout2{    
  width: 100%; 
  position: relative;
  vertical-align: top;
  padding: 10rpx; 
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.activity-item-layout2 image{
   height: 572rpx;
}
.layout2-price{  
    color: #e6282c;
    font-size: 32rpx;
}
.pl-24{
  padding-left: 24rpx;
}
.mb0{
  margin-bottom: 0;
}

/* layout4 */
.activity-item-layout4{
  padding: 10rpx;
    background-color: #fff;
    margin-bottom: 16rpx;
    border: 1rpx solid #E9E9E9;
    display: -webkit-box;
}
.activity-item-layout4 .item-inner{
  position: relative;
    width: 176rpx;
    height: 176rpx;
    border: none 0;
