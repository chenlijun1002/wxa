/* pages/detail/detail.wxss */
@import '../mycoupon/mycoupon.wxss';
.box{
  width: 100%;
  overflow-x: hidden;
}
.detail-content{
  position: relative;
}
swiper{
  height: 750rpx;
}
page{
  padding-bottom: 100rpx;
}
swiper image{
  width: 100%;
  height: 750rpx;
}
.swiper-text-info{
  background-color: #fff;
  padding: 10rpx 22rpx 30rpx;
}
.swiper-text-info .product-name{
  padding-right: 80rpx;
  position: relative;
  margin-bottom: 10rpx;
  font-size: 36rpx;
  line-height: 56rpx;
}
.swiper-text-info .product-name .share-icon{
  position: absolute;
  right: 0;
  top: 12rpx;
  width: 44rpx;
  height: 44rpx;
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/share-icon.png") no-repeat left top;
  background-size: cover;
}
text.lh0 {
  line-height: 40rpx;
}
.product-price .present-price{
  font-size: 40rpx;
  color: #00A699;
  float: left;
  margin-right: 16rpx;
}
.product-price .present-price text{
  font-size: 28rpx;
}
.product-price .original-price{
  float: left;
  font-size: 28rpx;
  color: #757575;
  margin-top: 10rpx;
  text-decoration: line-through;
}
.product-price .sales-volume{
  font-size: 28rpx;
  color: #757575;
  float: right;
  margin-top: 8rpx;
}
.product-price  .return-commission {
  margin: 10rpx 0 0 24rpx;
}
.product-configure{
  padding-left: 24rpx;
  background-color: #fff;
  margin-bottom: 24rpx;
  overflow: hidden;
}
.configure-row{
  position: relative;
  padding: 32rpx 80rpx 32rpx 130rpx;
  font-size: 32rpx;
  line-height: 48rpx;
}
.configure-row .configure-label{
  position: absolute;
  left: 0;
  top: 34rpx;
  color: #757575;
  font-size: 32rpx;
  line-height: 44rpx;
}
.configure-row .specifications-text{
  min-height: 44rpx;
}
.configure-row .address .address-icon{
  float: left;
  width:26rpx;
  height: 32rpx;
  margin-top: 9rpx;
  margin-right: 10rpx;
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/address-icon.png") no-repeat left top;
  background-size: cover;
}
.product-configure .right-icon{
  position: absolute;
  width: 40rpx;
  height: 8rpx;
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/right-icon.png") no-repeat left top;
  background-size: cover;
  top: 48rpx;
  right: 24rpx;
}
.nav-fiexd-bottom-item.nav-icon{
  width: 12%;
}
button.nav-fiexd-bottom-item{
    padding: 0;
    height: 100rpx;
    line-height:normal;
    background-color: transparent;
}
.nav-fiexd-bottom-item .icon{
  position: relative;
  display: block;
  width: 44rpx;
  height: 44rpx;
  margin: 12rpx auto 8rpx;
}
.nav-fiexd-bottom-item .icon .badge{
  position: absolute;
  right: -6rpx;
  top: -6rpx;
}
.nav-fiexd-bottom-item .icon .addnum{
  position: absolute;
  right: -6rpx;
  top: -6rpx;
  color: #FA3F6E;
  font-size: 26rpx;
  font-weight: 700;
  opacity: 0;
}
.nav-fiexd-bottom-item .icon .addnum.show{
  animation: addnum 1s step-end;
}
@keyframes addnum{
  0%{
    opacity: 0;
    top: -6rpx;
  }
  25%{
    opacity: 0.5;
    top: -28rpx;
  }
  50%{
    opacity: 1;
    top: -50rpx;
  }
  75%{
    opacity: 0.5;
    top: -28rpx;
  }
  100%{
    opacity: 0;
    top: -6rpx;
  }
}
.nav-fiexd-bottom-item .index-icon{
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/nav-bottom-index.png") no-repeat center bottom;
  background-size: 95%;
}
.nav-fiexd-bottom-item .message-icon{
  background: url("http://file.xiaokeduo.com/system/xkdxcx/system/images/message-icon.png") no-repeat center bottom;
  background-size: 100%;
}
.nav-fiexd-bottom-item .cart-icon{
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/nav-bottom-cart.png") no-repeat left center;
  background-size: auto 100%;
}
.nav-fiexd-bottom-item .icon-text{
  display: block;
  font-size: 24rpx;
  text-align: center;
  color: #202020;
  height: 24rpx;
  line-height: 24rpx;
}
.nav-fiexd-bottom .operation-btn{
  width: 32%;
}
.nav-fiexd-bottom .operation-btn text{
  display: block;
  line-height: 100rpx;
  height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}
.operation-btn.add-cart{
  background: linear-gradient(to right, #ffcc66 , #ff8947);
}
.operation-btn.buy-now{
  background: linear-gradient(to right, #ff6e47, #ff4757);
}
.rule-popup{
  position: fixed;
  width: 100%;
  height: 880rpx;
  left: 0;
  bottom: 0;
  background-color: #fff;
  z-index: 20;
  transform: translateY(120%);
  transition: all 0.4s;
}
.rule-popup .rule-popup-herder{
  position: relative;
  padding: 24rpx 0 46rpx 248rpx;
  border-bottom: 1px solid #DFDFDF;
  min-height: 200rpx;
  box-sizing: border-box;
}
.rule-popup .rule-popup-herder .price{
  margin-bottom: 20rpx;
}
.rule-popup .rule-popup-herder .price text:nth-child(2){
  font-size: 40rpx;
  margin-right: 28rpx;
}
.selected-specifications{
  height: 42rpx;
  line-height: 42rpx;
}
.rule-popup .rule-popup-herder .product-imag{
  position: absolute;
  left: 24rpx;
  top: -30rpx;
  width: 202rpx;
  height: 202rpx;
  border-radius: 8rpx;
  border:1px solid #DFDFDF;
}
.rule-popup .rule-popup-herder .cancel{
  position: absolute;
  right: 24rpx;
  top: 24rpx;
}
.rule-popup-body{
  padding: 0 24rpx;
}
.specifications-list{
  margin-top: 20rpx;
  margin-bottom: 34rpx;
}
.specifications-list .specifications-title{
  font-size: 32rpx;
  margin-bottom: 24rpx;
}
.specifications-list-group .specifications-item{
  display: inline-block;
  font-size: 24rpx;
  padding:0 20rpx;
  background-color: #F5F5F5;
  border-radius: 6rpx;
  color: #757575;
  margin-right: 32rpx;
  min-width: 40rpx;
  text-align: center;
  transition: all 0.4s;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  height: 64rpx;
  line-height: 64rpx;
}
.specifications-list-group .specifications-item.select{
  background-color: #FA3F6E;
  color: #fff;
}
.buy-number{
  margin: 56rpx 0;
}
.buy-number .number-text{
  float: left;
  font-size: 32rpx;
  color: #202020;
  line-height: 64rpx;
  margin-right: 30rpx;
}
.buy-number .number-control text{
  float: left;
  width: 64rpx;
  height: 64rpx;
  text-align: center;
  line-height: 66rpx;
  color: #999;
  border-radius: 4rpx;
