}
.rule-popup-footer {
  position: absolute;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  width: 100%;
  height: 96rpx;
}
.rule-popup-footer button{
  width: 100%;
  height: 100%;
  line-height: 96rpx;
  border-radius: 0;
  color: #fff;
  background: linear-gradient(to right, #ff6e47 , #ff4757);
}
.btn-hover{
  background-color: #ED5F83 !important;
  color: #fff;
}
.rule-popup.show{
  transform: translateY(0);
}
.image-text-title{
