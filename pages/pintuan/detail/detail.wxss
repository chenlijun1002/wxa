/* pages/pintuan/detail/detail.wxss */
page{
  padding-bottom: 100rpx;
}
swiper {
  width: 100%;
  height: 750rpx;
}

swiper image {
  width: 100%;
  height: 750rpx;
}
.swiper-bottom{
  height: 114rpx;
}
.swiper-table-view{
  display: table;
  width: 100%;
}
.swiper-table-view .table-view-cell,.end-date-time .date-time-content{
  display: table-cell;
  vertical-align: middle;
}
.end-date-time{
  display: table;
  width: 100%;
  text-align: center;
  height: 114rpx;
  color: #fff;
  background: #E3353B;
  background: -moz-linear-gradient(left,  #E44031 0%, #E3343C 100%);
  background: -webkit-gradient(linear, left center, right center, color-stop(0%,#E44031), color-stop(100%,#E3343C));
  background: -webkit-linear-gradient(left,  #E44031 0%,#E3343C 100%);
  background: -ms-linear-gradient(left,  #E44031 0%,#E3343C 100%);
  background: linear-gradient(to right,  #E44031 0%,#E3343C 100%);
}
.end-date-time .end-text{
  margin: 0;
  padding: 0;
  font-size: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  margin-bottom: 10rpx;
  letter-spacing: 2rpx;
}
.end-date-time .time{
  font-size: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.swiper-table-left{
  background: #FF7800;
  background: -moz-linear-gradient(left,  #FF7800 0%, #FB4B2B 100%);
  background: -webkit-gradient(linear, left center, right center, color-stop(0%,#FF7800), color-stop(100%,#FB4B2B));
  background: -webkit-linear-gradient(left,  #FF7800 0%,#FB4B2B 100%);
  background: -ms-linear-gradient(left,  #FF7800 0%,#FB4B2B 100%);
  background: linear-gradient(to right,  #FF7800 0%,#FB4B2B 100%);
  color: #fff;
}
.swiper-table-left .price{
  position: relative;
  padding-left: 26rpx;
  font-size: 48rpx;
  margin-right: 20rpx;
  float: left;
  height: 75rpx;
  line-height: 75rpx;
  padding-right: 20rpx;
  border-right: 1rpx solid #fff;
}
.original-price-num{
  float: left;
  font-size: 28rpx;
  height: 70rpx;
  padding-top: 4rpx;
}
.original-price-num .num,.original-price-num .original-price{
  font-size: 28rpx;
  line-height: 28rpx;
}
.original-price-num .num{
  margin-bottom: 16rpx;
}
.original-price-num .original-price{
  text-decoration:line-through;
}
.activity-prodcut-name{
  padding: 35rpx 23rpx;
  background-color: #fff;
  position: relative;
}
.activity-prodcut-name .name{
  display: block;
  font-size: 36rpx;
  line-height: 52rpx;
  padding-right: 60rpx;
  vertical-align: top;
}
.share-icon{
  position: absolute;
  right: 0rpx;
  top: 15rpx;
  width: 90rpx;
  height: 90rpx;
  background: url("http://testfile.xiaokeduo.com/system/xkdxcx/system/images/share-icon.png") no-repeat center center;
  background-size: 44rpx 44rpx;
}
.prodcut-specifications{
  height: 106rpx;
  background-color: #F7F7F7;
}
.select-rows.prodcut-specifications .select-text{
  line-height: 106rpx;
}
.section-box{
  padding-bottom: 40rpx;
}
.activity-rule{
  padding: 0 24rpx;
}
.activity-rule .step{
  position: relative;
  float: left;
  padding-left: 70rpx;
  margin: 40rpx 0;
}
.activity-rule .step .progress-number{
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  width: 48rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  border:1px solid #515151;
  border-radius: 50%;
  color: #515151;
  font-size: 28rpx;
}
.activity-rule .step .step-text{
  font-size: 28rpx;
  line-height: 42rpx;
  color: #515151;
}
.activity-rule .arrow{
  float: left;
  width: 21rpx;
  height: 23rpx;
  margin: 71rpx 19rpx;
  background: url('http://xkdsaas.oss-cn-shanghai.aliyuncs.com/system/xkdxcx/system/images/arrow.png') no-repeat left top;
  background-size: cover;
}
.activity-rule-rompt{
  font-size: 28rpx;
  color: #747474;
  padding-left: 26rpx;
}
.activity-rule-rompt label{
  padding: 4rpx 8rpx;
  font-size: 24rpx;
  color: #FD1D1D;
  border:1px solid #FD1D1D;
  border-radius: 8rpx;
  margin-right: 10rpx;
}
.open-group-ing .table-view-cell .media-object{
  width: 106rpx;
  height: 106rpx;
  border-radius: 8rpx;
}
.open-group-ing .table-view-cell .media-body{
  height: auto;
  padding-bottom: 24rpx;
  border-bottom: 1rpx solid #E0E0E0;
  overflow-x: hidden;
  overflow-y: visible;
}
.open-group-ing .table-view-table:last-child .table-view-cell .media-body{
  border-bottom: none;
}
.open-group-ing .media-body .product-name{
  margin-bottom: 10rpx;
}
.open-group-ing .table-view-table{
  padding:20rpx 24rpx 0;
}
.open-group-ing .participate-in{
  position: absolute;
  right: 0;
  bottom: 24rpx;
  color: #FF7800;
  padding: 0 10rpx;
  border:1rpx solid #FF7800;
  border-radius: 8rpx;
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.nav-fiexd-bottom-item.nav-icon{
  width: 12%;
}
.nav-fiexd-bottom-item .icon{
  position: relative;
  display: block;
  width: 44rpx;
  height: 44rpx;
  margin: 12rpx auto 8rpx;
}
.nav-fiexd-bottom-item .index-icon{
  background: url("http://xkdsaas.oss-cn-shanghai.aliyuncs.com/system/xkdxcx/system/images/nav-special.png") no-repeat center bottom;
  background-size: 100%;
}
.nav-fiexd-bottom-item .member-icon{
  background: url("http://xkdsaas.oss-cn-shanghai.aliyuncs.com/system/xkdxcx/system/images/nav-member.png") no-repeat left center;
  background-size: auto 100%;
}
.nav-fiexd-bottom-item .icon-text{
  display: block;
  font-size: 24rpx;
  text-align: center;
  color: #202020;
  height: 24rpx;
  line-height: 24rpx;
}
.nav-fiexd-bottom .operation-btn{
    width: 64%;
  display: block;
  line-height: 100rpx;
  height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  border-radius: 0;
}
.operation-btn.go-open-group{
  background: #FFA200;
  background: -moz-linear-gradient(left,  #FFA200 0%, #FF7800 100%);
  background: -webkit-gradient(linear, left center, right center, color-stop(0%,#FFA200), color-stop(100%,#FF7800));
  background: -webkit-linear-gradient(left,  #FFA200 0%,#FF7800 100%);
  background: -ms-linear-gradient(left,  #FFA200 0%,#FF7800 100%);
  background: linear-gradient(to right,  #FFA200 0%,#FF7800 100%);
}
.operation-btn.go-open-group.disable-btn{
    background: #bbb;
}
.rule-popup{
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  background-color: #fff;
  z-index: 20;
  transform: translateY(120%);
  transition: all 0.4s;
}
.rule-popup .rule-popup-herder{
  position: relative;
  padding:20rpx 0 20rpx 218rpx;
  border-bottom: 1px solid #DFDFDF;
  min-height: 165rpx;
  box-sizing: border-box;
}
.rule-popup .rule-popup-herder .price{
  margin-bottom: 20rpx;
  color: #FD1D1D;
  font-size: 40rpx;
}
.rule-popup .rule-popup-herder .price text{
  font-size: 28rpx;
}
.selected-specifications{
  font-size: 32rpx;
  height: 42rpx;
  line-height: 42rpx;
}
.rule-popup .rule-popup-herder .product-imag{
  position: absolute;
  left: 24rpx;
  top: -30rpx;
  width: 162rpx;
  height: 162rpx;
  border-radius: 8rpx;
  border:1px solid #DFDFDF;
  background-color: #fff;
}
.rule-popup .rule-popup-herder .cancel{
  position: absolute;
  right: 24rpx;
  top: 24rpx;
}
.rule-popup-body{
  padding: 0 24rpx;
}
.specifications-list{
  margin-top: 20rpx;
  margin-bottom: 34rpx;
}
.specifications-list .specifications-title{
  font-size: 32rpx;
  margin-bottom: 24rpx;
}
.specifications-list-group .specifications-item{
  display: inline-block;
  font-size: 24rpx;
  padding: 14rpx 35rpx;
  background-color: #F5F5F5;
  border-radius: 20rpx;
  color: #757575;
  margin-right: 24rpx;
  min-width: 40rpx;
  text-align: center;
  transition: all 0.4s;
  margin-bottom: 24rpx;
}
.specifications-list-group .specifications-item.select{
  background-color: #FFA200;
  color: #fff;
}
.buy-number{
  margin: 56rpx 0;
}
.buy-number .number-text{
  float: left;
  font-size: 32rpx;
  color: #202020;
